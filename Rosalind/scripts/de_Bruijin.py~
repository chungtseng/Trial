

if __name__ == '__main__':
    S = []
    with open('Rosalind/input/dbj_in.txt','rb') as in_data:
        for line in in_data:
            S.append(line.strip())
    bound = dict(zip('ATCG','TAGC'))
    rc = lambda seq, bound: [bound[i] for i in seq]
    Src = map(rc, S)
    S = set(S)
    Src = set(Src)
    s_t = S + Src
    
